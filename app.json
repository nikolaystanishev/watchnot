{
  "expo": {
    "name": "watchnot",
    "slug": "watchnot",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      },
      "bundleIdentifier": "com.watchnot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.watchnot",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "minify": false
    }
  }
}
